  <div class="w-nav" data-collapse="medium" data-animation="default" data-duration="400" data-contain="1">
  </div>
  <h1 class="browseheading">Browse</h1>
  <div class="row">

  <input style="margin-left:61%;height:20px;box-shadow: 0 0 5px rgb(147, 197, 189);border: 1px solid rgb(147,192,189);" placeholder="Search" size="12" type="text" onkeyup="filter(this)"/>
</div>
  <br>
  <div class="row">
  {{#each group}}
        {{#each item}}
        <div class="col-4 col-xs-4 name" style="min-height: 170px;">
          <a href="itemdetail/{{name}}"><img src={{imageURL}} /></a>
            <center><a style="font-family:Bitter;"href="itemdetail/{{name}}">{{name}} </a></center>
        </div>
      {{/each}}
  {{/each}}
</div>

<script language="javascript" type="text/javascript">
    function filter(element) {

        var value = $(element).val().toLowerCase();

        var string = document.body.innerHTML.toLowerCase();

        $(".name").each(function() {
            if ($(this).text().toLowerCase().search(value) > -1) {
                $(this).show();
            }
            else {
                $(this).hide();
            }
        });
    }


   $(document).ready(function() { 
      checkCookie();
    });
</script>